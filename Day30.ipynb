   "source": [
    "#swapping\n",
    "l = [2,5,12,13,25,43]\n",
    "i=0\n",
    "j= len(l)-1\n",
    "while(i<=j):\n",
    "    a=l[i]\n",
    "    l[i]=l[j]\n",
    "    l[j]=a\n",
    "    i=i+1\n",
    "    j=j-1\n",
    "print(l)\n",
    "    "
   ]
  },
